    "@types/node": "^20.10.5",
    "jest": "^30.0.0",
    "ts-jest": "^29.3.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sw3do/quick-prisma.git"
